{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "ES2016", "ES2021"],
    "declaration": true,
    "declarationDir": "dist/types",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "jsx": "react",
    "resolveJsonModule": true,
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "types": ["vite/client","node"],
    "baseUrl": "src/"
  },
  "include": [".", "src", "src/Home/.tsx"],
  "exclude": ["dist/**", "src/main.tsx"]
}
